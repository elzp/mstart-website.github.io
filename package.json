{
  "name": "birdy-page",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://elzp.github.io/music-star-website",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.8"
  },
  "scripts": {
    "start": "parcel ./public/index.html",
    "prebuild-dev": "shx rm -rf dist/*",
    "build-dev": "parcel build ./public/index.html --no-optimize --public-url ./",
    "prebuild": "shx rm -rf docs/*",
    "build": "parcel build ./public/index.html --public-url ./ --dist-dir static"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "shx": "^0.3.4"
  },
  "repository": "https://github.com/elzp/music-star-website.git",
  "author": "<e-l-a-p@wp.pl>",
  "license": "MIT"
}
